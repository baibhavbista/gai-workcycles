{
  "name": "workcycles-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.ts",
  "scripts": {
    "electron:dev": "concurrently --kill-others-on-fail \"NODE_ENV=dev vite --port 5173 --strictPort\" \"NODE_ENV=dev electron --trace-warnings -r esbuild-register electron/main.ts\"",
    "electron:build": "vite build && electron-builder",
    "typecheck:electron": "tsc -p tsconfig.electron.json --noEmit",
    "rebuild:native": "electron-rebuild -f -w better-sqlite3",
    "postinstall": "npm run rebuild:native && electron-builder install-app-deps"
  },
  "dependencies": {
    "@lancedb/lancedb": "^0.21.0",
    "@langchain/langgraph": "^0.3.6",
    "@tanstack/react-table": "^8.10.7",
    "apache-arrow": "^18.1.0",
    "better-sqlite3": "^12.2.0",
    "lucide-react": "^0.344.0",
    "openai": "^5.8.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "uuid": "^11.1.0",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "electron": "^27.0.0",
    "@eslint/js": "^9.9.1",
    "@types/better-sqlite3": "^7.6.6",
    "@types/node": "^20.10.11",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "electron-builder": "^24",
    "electron-rebuild": "^3.2.9",
    "esbuild-register": "^3.4.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2"
  }
}
